<?xml version='1.0'?>
<sdf version='1.4'>
    <model name='puzzlebot1' canonical_link='chassis'>
    
    
        <!--chassis-->
        <link name='chassis'>
            <pose relative_to='__model__'>0 0 0.05 0 0 0</pose>
            
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>0.8</mass>
                <pose>-0.05 0 0 0 0 0</pose>
                <inertia>
                    <ixx>0.004</ixx>
                    <iyy>0.004</iyy>
                    <izz>0.004</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/chassis1.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>-0.055 0 0 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.2 0.16 0.046</size>
                    </box>
                </geometry>
            </collision>
            
            <sensor type="camera" name="camera_sensor">
                <pose relative_to='camera_frame'>0 0 0 0 0 0</pose>
                <topic>camera</topic>
                <camera>
                    <horizontal_fov>1.047</horizontal_fov>
                    <image>
                        <width>1280</width>
                        <height>720</height>
                    </image>
                    <clip>
                        <near>0.1</near>
                        <far>100</far>
                    </clip>
                </camera>
                <always_on>1</always_on>
                <update_rate>10</update_rate>
                <visualize>false</visualize>
            </sensor>
            <sensor name='gpu_lidar' type='gpu_lidar'>"
                <pose relative_to='lidar_frame'>0 0 0 0 0 0</pose>
                <topic>lidar</topic>
                <update_rate>10</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>1146</samples>
                            <resolution>1</resolution>
                            <min_angle>-3.14</min_angle>
                            <max_angle>3.14</max_angle>
                        </horizontal>
                        <vertical>
                            <samples>1</samples>
                            <resolution>0.01</resolution>
                            <min_angle>0</min_angle>
                            <max_angle>0</max_angle>
                        </vertical>
                    </scan>
                    <range>
                        <min>0.08</min>
                        <max>10.0</max>
                        <resolution>0.01</resolution>
                    </range>
                </ray>
                <always_on>1</always_on>
                <visualize>true</visualize>
            </sensor>
        </link>


        <!--Right wheel-->
        <link name='right_wheel'>
            <pose relative_to="chassis">0.0 -0.095 0 0 0 3.1415</pose> <!--angles are in radian-->
            
            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.002</ixx>
                    <iyy>0.002</iyy>
                    <izz>0.002</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>0 -0.0025 0 1.5707 0 0</pose> <!--angles are in radian-->
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.005</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
            
        </link>


        <!--Left wheel-->
        <link name='left_wheel'>
            <pose relative_to="chassis">0.0 0.095 0 0 0 0</pose> <!--angles are in radian-->
            
            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.002</ixx>
                    <iyy>0.002</iyy>
                    <izz>0.002</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>0 -0.0025 0 1.5707 0 0</pose> <!--angles are in radian-->
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.005</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
            
        </link>


        <!--arbitrary frame-->
        <frame name="caster_frame" attached_to='chassis'>
            <pose>-0.145 0 -0.044 0 0 0</pose>
        </frame>


        <!--caster wheel-->
        <link name='caster'>
            <pose relative_to='caster_frame'/>
            
            <inertial>
                <mass>0.01</mass>
                <inertia>
                    <ixx>0.0000025</ixx>
                    <iyy>0.0000025</iyy>
                    <izz>0.0000025</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.006</radius>
                    </sphere>
                </geometry>
                <material>
                    <ambient>0.0 1 0.0 1</ambient>
                    <diffuse>0.0 1 0.0 1</diffuse>
                    <specular>0.0 1 0.0 1</specular>
                </material>
            </visual>
            
            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.006</radius>
                    </sphere>
                </geometry>
            </collision>
        </link>


        <!--left wheel joint-->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel'/>
            <parent>chassis</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>1</effort>
                </limit>
               <dynamics>
                   <friction>0.1</friction>
               </dynamics>
            </axis>
        </joint>


        <!--right wheel joint-->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel'/>
            <parent>chassis</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>1</effort>
                </limit>
               <dynamics>
                   <friction>0.1</friction>
               </dynamics>
            </axis>
        </joint>


        <!--caster wheel joint--> <!--pose defult value is the child-->
        <joint name='caster_wheel' type='ball'>
            <parent>chassis</parent>
            <child>caster</child>
        </joint>
                  
        <!-- Puzzlebot differential drive plugin -->
