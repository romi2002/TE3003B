<?xml version='1.0'?>
<sdf version='1.4'>
    <model name='puzzlebot1' canonical_link='chassis'>
    
    
        <!--chassis-->
        <link name='chassis'>
            <pose relative_to='__model__'>0 0 0.05 0 0 0</pose>
            
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>0.8</mass>
                <pose>-0.05 0 0 0 0 0</pose>
                <inertia>
                    <ixx>0.004</ixx>
                    <iyy>0.004</iyy>
                    <izz>0.004</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/chassis1.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>-0.055 0 0 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.2 0.16 0.046</size>
                    </box>
                </geometry>
            </collision>
            
        </link>


        <!--Right wheel-->
        <link name='right_wheel'>
            <pose relative_to="chassis">0.0 -0.095 0 0 0 3.1415</pose> <!--angles are in radian-->
            
            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.002</ixx>
                    <iyy>0.002</iyy>
                    <izz>0.002</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>0 -0.0025 0 1.5707 0 0</pose> <!--angles are in radian-->
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.005</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
            
        </link>


        <!--Left wheel-->
        <link name='left_wheel'>
            <pose relative_to="chassis">0.0 0.095 0 0 0 0</pose> <!--angles are in radian-->
            
            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.002</ixx>
                    <iyy>0.002</iyy>
                    <izz>0.002</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>./meshes/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>
            
            <collision name='collision'>
                <pose>0 -0.0025 0 1.5707 0 0</pose> <!--angles are in radian-->
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.005</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
            
        </link>


        <!--arbitrary frame-->
        <frame name="caster_frame" attached_to='chassis'>
            <pose>-0.145 0 -0.044 0 0 0</pose>
        </frame>


        <!--caster wheel-->
        <link name='caster'>
            <pose relative_to='caster_frame'/>
            
            <inertial>
                <mass>0.01</mass>
                <inertia>
                    <ixx>0.0000025</ixx>
                    <iyy>0.0000025</iyy>
                    <izz>0.0000025</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
            
            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.006</radius>
                    </sphere>
                </geometry>
                <material>
                    <ambient>0.0 1 0.0 1</ambient>
                    <diffuse>0.0 1 0.0 1</diffuse>
                    <specular>0.0 1 0.0 1</specular>
                </material>
            </visual>
            
            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.006</radius>
                    </sphere>
                </geometry>
            </collision>
        </link>


        <!--left wheel joint-->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel'/>
            <parent>chassis</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>1</effort>
                </limit>
               <dynamics>
                   <friction>0.1</friction>
               </dynamics>
            </axis>
        </joint>


        <!--right wheel joint-->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel'/>
            <parent>chassis</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                    <effort>1</effort>
                </limit>
               <dynamics>
                   <friction>0.1</friction>
               </dynamics>
            </axis>
        </joint>


        <!--caster wheel joint--> <!--pose defult value is the child-->
        <joint name='caster_wheel' type='ball'>
            <parent>chassis</parent>
            <child>caster</child>
        </joint>
                  
        <!-- Puzzlebot differential drive plugin -->
        <plugin name="DiffDynamicPlugin" filename="libDiffDynamicPlugin.so">
            <leftJoint>left_wheel_joint</leftJoint>
            <rightJoint>right_wheel_joint</rightJoint>
            <cmd_R_topic>ControlR</cmd_R_topic>
            <cmd_L_topic>ControlL</cmd_L_topic>
            <cmd_vel_topic>cmd_vel</cmd_vel_topic>
            <enc_R_topic>VelocityEncR</enc_R_topic>
            <enc_L_topic>VelocityEncL</enc_L_topic>
            <mot_Vmax>5</mot_Vmax>
            <mot_J>0.0005</mot_J>
            <mot_R>2</mot_R>
            <mot_L>0.01</mot_L>
            <mot_K>0.01</mot_K>
            <mot_b>0.00001</mot_b>
            <mot_gear>34</mot_gear>
            <robot_width>0.09</robot_width>
            <wheel_radius>0.05</wheel_radius>
            <pid_Kp>0.1</pid_Kp>
            <pid_Ti>0.05</pid_Ti>
            <pid_Td>0</pid_Td>
            <input_type>3</input_type>
            <rate>100</rate>
        </plugin>
                
    </model>

</sdf>

